{
    "name": "ontario/zend-ogame",
    "description": "Ogame game clone.",
    "license": "BSD-3-Clause",
    "keywords": [
	    "ogame",
        "framework",
        "zf2"
    ],
    "homepage": "http://ogame.ontario.pp.ua/",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/ontario/DoctrineDataFixtureModule.git"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.3.*",
        "zf-commons/zfc-user-doctrine-orm": "1.0.0",
        "zf-commons/zfc-rbac": "2.3.1",
        "zf-commons/zfc-twig": "dev-master",
        "zendframework/zend-developer-tools": "dev-master",
        "bjyoungblood/bjy-profiler": "v1.1.0",
        "ocramius/ocra-service-manager": "2.1.1",
        "ocramius/generated-hydrator": "1.0.2",
        "zendframework/zftool": "dev-master",
        "sensiolabs/security-checker": "dev-master",
        "doctrine/data-fixtures": "dev-master",
        "ontario/doctrine-data-fixture-module": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit" : "3.7.*",
        "satooshi/php-coveralls": ">=0.6.0",
        "squizlabs/php_codesniffer": "1.4.*"   
    },
    "autoload": {
        "psr-0": {
            "Application": "src/",
    	    "Auth": "src/",
    	    "Admin": "src/",
    	    "Game": "src/"
        }
    }

}
